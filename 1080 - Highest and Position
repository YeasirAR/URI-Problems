#include<stdio.h>
main()
{
    int i,max,pos;
    long int n[99];
    for(i=0; i<100; i++)
    {
        scanf("%d",&n[i]);
    }
    max=n[0];
    pos=1;
    for(i=1; i<100; i++)
    {
        if(n[i]>max)
        {
            max=n[i];
            pos=i+1;
        }
    }
    printf("%d\n%d\n",max,pos);

    return 0;
}
